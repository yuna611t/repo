@model PageModel


<main class="col-lg-9 col-md-9 col-sm-8">
	<section>
		<div class="well">
			@Html.Raw(Model.Description)
		</div>
		<div class="well">
			WebSiteURL : @CommonFunctionalities.getWebSiteURL()
		</div>
	</section>

	@if (Model.Tabs != null) {
		@Html.Partial("~/Views/Contents/Tabs.cshtml", Model.Tabs)
	}

</main>
